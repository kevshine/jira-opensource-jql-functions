<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="${project.groupId}.${project.artifactId}" name="${project.name}" plugins-version="2">
  <plugin-info>
    <description>${project.description}</description>
    <version>${project.version}</version>
    <vendor name="${project.organization.name}" url="${project.organization.url}"/>
  </plugin-info>

    <jql-function name="Recent Project Function2" i18n-name-key="parent-fixversion-in-function.name" key="parent-fixversion-in-function" class="com.opensource.plugins.jira.jql.ParentsWithAFixVersionOf">
    <fname>FixVersionOf</fname>
    <description key="parent-fixversion-in-function.description">Provides a list of all issues that have parent issues with a fix version as entered</description>
    <list>true</list>
  </jql-function>
  
    <jql-function name="Recent Project Function2" i18n-name-key="parent-status-in-function.name" key="parent-status-in-function" class="com.opensource.plugins.jira.jql.ParentStatusIn">
    <fname>CurrentStatusOf</fname>
    <description key="parent-status-in-function.description">Provides a list of all issues that have parent issues with a parent status as entered</description>
    <list>true</list>
  </jql-function>
  
  <jql-function name="Recent Project Function" i18n-name-key="recent-project-function.name" key="recent-project-function" class="com.opensource.plugins.jira.jql.RecentProjectFunction">
    <fname>recentProjects</fname>
    <description key="recent-project-function.description">Provides a JQL function to restrict the search to recently visited projects.</description>
    <list>true</list>
  </jql-function>
  
    
  <resource type="i18n" name="i18n" location="atlassian-plugin"/>
</atlassian-plugin>
